# API Configuration
VITE_API_URL=/api
VITE_API_TIMEOUT=10000

# App Configuration
VITE_APP_NAME=HealthBridge
VITE_APP_VERSION=1.0.0

# Features
VITE_ENABLE_REGISTRATION=true
VITE_ENABLE_PASSWORD_RESET=true

# Microservicios (dev targets)
# Auth Service
VITE_AUTH_SERVICE_TARGET=http://localhost:3001
# User Service
VITE_USER_SERVICE_TARGET=http://localhost:3002
# Appointment Service
VITE_APPOINTMENT_SERVICE_TARGET=http://localhost:3003
# Notification Service
VITE_NOTIFICATION_SERVICE_TARGET=http://localhost:3004
# Billing Service
VITE_BILLING_SERVICE_TARGET=http://localhost:3006
# Subscription Service
VITE_SUBSCRIPTION_SERVICE_TARGET=http://localhost:3007

